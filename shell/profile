# Path definitions
export PATH=/usr/local/bin:$PATH
export PATH=/$PATH:/usr/local/sbin
# Node Path
export PATH=$PATH:/usr/local/Cellar/nvm/0.32.1/versions/node/v0.12.2/bin
# Variable definitions
export POWERLINE_PATH=/usr/local/lib/python2.7/site-packages/powerline
export NVM_DIR="$(brew --prefix nvm)"; [ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"
